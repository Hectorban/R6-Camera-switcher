{
  "name": "R6-Camera-switcher",
  "version": "2.0.0",
  "description": "nodecg react parcel template",
  "homepage": "https://github.com/Hectorban/Nodecg-parcel-template",
  "author": "Alysanne <hectorban1234@gmail.com> (http://alysanne.dev/)",
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "src",
    "extension"
  ],
  "keywords": [
    "template",
    "react",
    "nodecg-bundle"
  ],
  "scripts": {
    "start": "start npm run watchDashboard && start npm run watchGraphics && start npm run watchExtension",
    "watchDashboard": "parcel watch src/dashboard/*.html --out-dir dashboard --public-url ./",
    "watchGraphics": "parcel watch src/graphics/*.html --out-dir graphics --public-url ./",
    "watchExtension": "parcel watch src/extension/index.ts --out-dir extension --target node --bundle-node-modules",
    "buildDashboard": "parcel build src/dashboard/*.html --out-dir dashboard --public-url ./",
    "buildGraphics": "parcel build src/graphics/*.html --out-dir graphics --public-url ./",
    "buildExtension": "parcel build src/extension/index.ts --out-dir extension --target node --bundle-node-modules",
    "build": "start npm run buildDashboard && start npm run buildGraphics && start npm run buildExtension"
  },
  "externals": [
    "iohook"
  ],
  "iohook": {
    "targets": [
      "node-83"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "nodecg": {
    "compatibleRange": "^1.1.1",
    "dashboardPanels": [
      {
        "name": "Rainbow 6 SWITCHER",
        "title": "Control panel",
        "file": "Control_panel.html",
        "headerColor": "#525F78",
        "fullbleed": true
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "index2.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "index3.html",
        "width": 1920,
        "height": 1080
      }
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "2.2.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "flux": "^4.0.1",
    "formik": "^2.2.9",
    "iohook": "^0.6.6",
    "parcel-plugin-externals": "^0.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.13.8",
    "react-select": "^4.3.1",
    "regenerator-runtime": "^0.13.7"
  }
}
